. // <
    . _# n $ the n-th fibonacci number $
    . # > n
    . _# r1
    . _# r2
    . ~<2>~ <
        . ?< |@==0| <
            . # arg1 = n-1
            . r1 = fib(arg1)
        > <
            . # arg2 = n-2
            . r2 = fib(arg2)
        >
    >
    . # < r1 + r2
    . * < '\n'
>

. # fib (# n) <
	. ?- |n <= 1| <
		. :: n
	>
    . # arg1 = n-1
    . # arg2 = n-2
	. # r = fib(arg1) + fib(arg2)
	. :: r
>
